{
    "problemMatcher": [
      {
        "owner": "bicep-linter-warning",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "(^[Ww].+) (/.+\\.bicep)\\((\\d+),(\\d+)\\)\\s:\\s(.*)$",
            "file": 2,
            "line": 3,
            "column": 4,
            "message": 5
          }
        ]
      }
    ]
  }
