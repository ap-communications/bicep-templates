{
    "bicep_linter_error": [
      {
        "owner": "AP Communications",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^ERROR: ([^:]+):(\\d+):(\\d+):\\s+(E\\d+\\s+.+)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4
          }
        ]
      }
    ]
  }